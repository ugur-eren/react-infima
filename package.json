{
  "name": "react-infima",
  "version": "0.0.1-alpha.4",
  "title": "React Infima",
  "description": "React components for infima. This doesn't add any new features to infima, just ports the components to react.",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ugur-eren/react-infima"
  },
  "bugs": {
    "url": "https://github.com/ugur-eren/react-infima/issues"
  },
  "keywords": [
    "front-end",
    "react",
    "infima",
    "web",
    "docusaurus"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn clean-build && yarn rollup-build",
    "rollup-build": "rollup -c",
    "clean-build": "rimraf dist",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "format:check": "prettier -c src/**/*.{ts,tsx}",
    "lint": "yarn lint:check --fix",
    "lint:check": "eslint src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "infima": ">=0.2.0-alpha.37",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^16.7.13",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "rollup": "^2.67.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  }
}
